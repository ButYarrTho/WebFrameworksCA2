<h3>Music</h3>
<button @onclick="DownloadArtists">Download</button>
<ol>

    @if (topartists != null)
    {
    @foreach (var artist in topartists.topartists.artist)
    {
    <li>
        @artist.name
    </li>
    }
    }
</ol>
@code {

private Artists? topartists;

private void DownloadArtists()
{
topartists = MusicService.GetTopArtists();
}

}